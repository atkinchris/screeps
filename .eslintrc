{
  "env": {
      "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "semi": ["error", "never"],
    "no-console": 0,
    "global-require": 0,
  },
  "globals": {
    "Game": false,
    "Memory": false,
    "WORK": false,
    "CARRY": false,
    "MOVE": false,
    "FIND_SOURCES": false,
    "ERR_NOT_IN_RANGE": false,
    "FIND_STRUCTURES": false,
    "STRUCTURE_EXTENSION": false,
    "STRUCTURE_SPAWN": false,
    "RESOURCE_ENERGY": false,
  }
}
